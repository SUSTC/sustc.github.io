{
  "title": "SUSTech GitBook",
  "root": "./src",
  "language": "zh-hans",
  "plugins": [
    "editlink",
    "splitter",
    "katex",
    "advanced-emoji",
    "tbfed-pagefooter"
  ],
  "pluginsConfig": {
    "editlink": {
      "base": "https://gitlab.com/sustc/gitbook/blob/master",
