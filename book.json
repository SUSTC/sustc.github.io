{
  "title": "SUSTech GitBook",
  "root": "./src",
  "language": "zh-hans",
  "plugins": [
    "custom-js-css",
    "splitter",
    "katex",
    "advanced-emoji",
    "page-footer-liziwl"
  ],
  "pluginsConfig": {
    "custom-js-css": {
      "js": [
        "./js/jquery.min.js",
        "./js/jquery.dataTables.min.js"
      ],
      "css": [
        "./css/jquery.dataTables.min.css"
      ]
    },
    "page-footer-liziwl": {
			"description": "Updated at",
			"signature": "SUSTech GitBook Team",
			"wisdom": "Save Time on looking up information.",
			"format": "yyyy-MM-dd hh:mm:ss",
			"copyright": "Copyright &copy SUSTech GitBook Team 2019",
			"timeColor": "#666",
      "copyrightColor": "#666",
			"utcOffset": "8",
			"isShowQRCode": true,
      "baseUri": "https://sustech.online/",
      "isShowIssues": false,
			"repo": "",
			"issueNum": "",
			"token": "",
      "style": "symmetrical"
		}
  }
}